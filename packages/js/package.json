  "devDependencies": {
    "@nftbox/contract": "workspace:*",
    "nodemon": "^2.0.21",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.73.3"
  }
}
